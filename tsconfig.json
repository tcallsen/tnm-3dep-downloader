{
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "outDir": "dist"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "moduleResolution":"node",
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}